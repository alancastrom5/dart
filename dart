void main(){
double num1 = 4;
  double num2 = 6;
  double num3 = 8;
  
  double media = (num1 + num2 + num3) / 3;
  
  String nomeAluno = "Nathan";
  print ("media: $media");
  
  if (media >= 6){
    print("o $nomeAluno foi aprovado,sua media foi: $media");
} else {
    print("O $nomeAluno foi reprovado $media");
}
    if (media >= 6){
    print("$nomeAluno foi aprovado, $media");
} else {
    print("O $nomeAluno foi reprovado $media");
}
}
